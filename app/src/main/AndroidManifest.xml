<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.dumposk129.create.stories.app" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.dumposk129.create.stories.app.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.dumposk129.create.stories.app.ReadsFragment"
            android:label="@string/title_activity_reads_fragment" >
        </activity>
        <activity
            android:name="com.dumposk129.create.stories.app.CreatesFragment"
            android:label="@string/title_activity_creates_fragment" >
        </activity>
        <activity
            android:name="com.dumposk129.create.stories.app.EditsFragment"
            android:label="@string/title_activity_edits_fragment" >
        </activity>
        <activity
            android:name="com.dumposk129.create.stories.app.QuestionnairesFragment"
            android:label="@string/title_activity_questionnaires_fragment" >
        </activity>
        <activity
            android:name="com.dumposk129.create.stories.app.SettingsFragment"
            android:label="@string/title_activity_settings_fragment" >
        </activity>
    </application>

</manifest>
